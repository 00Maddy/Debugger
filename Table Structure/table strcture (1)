  `plate_type` varchar(120) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `received_time` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `registration_time` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `sequence_number` varchar(80) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `status` varchar(50) DEFAULT NULL,
  `temperature` varchar(50) DEFAULT NULL,
  `updated_at` datetime(6) DEFAULT NULL,
  `updated_by` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `user_id` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_brand_arabic` varchar(150) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_color_arabic` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
