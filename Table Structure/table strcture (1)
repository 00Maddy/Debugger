  `vin` varchar(45) NOT NULL,
  `io` int(11) NOT NULL,
  `min` int(11) DEFAULT NULL,
  `max` int(11) DEFAULT NULL,
  `ioname` varchar(45) DEFAULT NULL,
  `createdBy` varchar(45) DEFAULT NULL,
  `createdDt` datetime DEFAULT NULL,
  `lastUpdBy` varchar(45) DEFAULT NULL,
  `lastUpdDt` datetime DEFAULT NULL,
  `status` tinyint(4) DEFAULT NULL,
  `unit` varchar(20) DEFAULT NULL,
  `device_imei_id` varchar(50) DEFAULT NULL,
  `device_name` varchar(50) DEFAULT NULL,
  `device_location` varchar(100) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

