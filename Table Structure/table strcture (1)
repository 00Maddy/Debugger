--
-- Stand-in structure for view `vw_vehicle`
--
CREATE TABLE `vw_vehicle` (
`companyId` varchar(45)
,`branchId` varchar(45)
,`userId` varchar(45)
,`vin` varchar(20)
,`plateNo` varchar(200)
,`vehicletype_vehicleType` varchar(45)
,`vehicleGroup` varchar(45)
,`vehiclemodel_model` varchar(50)
,`dateOfPurchase` date
,`defaultwarrantyExpiryDate` date
,`regNo` varchar(45)
,`regDate` date
,`regExpiryDate` date
,`insuranceNo` varchar(45)
,`insuranceExpiryDate` date
,`opreAre` varchar(100)
,`mileageInit` varchar(25)
,`mileageCur` varchar(25)
,`imgUrl` varchar(300)
,`iconUrl` varchar(300)
,`additionalWarranty` date
,`geoCnt` int(11)
,`lastUpdBy` varchar(45)
,`lastUpdDt` datetime
,`ioSetting` varchar(900)
,`contactNo` varchar(45)
,`fuelTanklit` varchar(45)
,`fuelTankmV` varchar(45)
,`ioDetails` varchar(900)
,`status` varchar(45)
,`hourMeterValue` varchar(45)
,`capacity` int(11)
,`expectedMileage` int(11)
,`active` tinyint(1)
,`lockStatus` int(1)
,`PreventiveMaintenance` varchar(45)
,`ScheduledMaintenance` varchar(45)
,`restDay` varchar(200)
,`commandsJson` varchar(1200)
,`overAllcmdStatus` int(1)
,`reverseSetting` varchar(25)
,`hierarchyStructure` varchar(50)
,`v_timeStamp` datetime
,`v_heartBeatEngine` tinyint(1)
,`v_gps` varchar(10)
,`v_gsm` int(11)
,`v_batteryVoltage` varchar(20)
,`v_powerSupply` varchar(10)
,`v_remarks` varchar(1200)
,`v_eventTimeStamp` datetime
,`v_serverTimeStamp` datetime
,`v_latitude` double
,`v_longitude` double
,`v_speed` int(11)
,`v_priority` int(11)
,`v_ioevent` varchar(3000)
,`v_bytesTrx` bigint(100)
,`v_Engine` tinyint(1)
,`v_tempSensor1` bigint(10)
,`v_tempSensor2` bigint(10)
,`v_tempSensor3` bigint(10)
,`v_tempSensor4` bigint(10)
,`v_odometer` bigint(10)
,`v_battery` bigint(10)
,`v_direction` int(5)
,`v_eventSource` int(11)
,`v_AI1` int(11)
,`v_AI2` int(11)
,`v_AI3` int(11)
,`v_AI4` int(11)
,`v_DI1` int(11)
,`v_DI2` int(11)
,`v_DI3` int(11)
,`v_DI4` int(11)
,`v_tags` varchar(1000)
,`contractor` varchar(45)
,`vehiclestatus` varchar(500)
,`PreventiveMaintenanceType` varchar(25)
,`vehicleStatusType` varchar(200)
,`EngineHours` bigint(100)
,`group_color` varchar(20)
,`eventStatus` varchar(100)
,`statusDuration` bigint(50)
,`totalEngineHours` bigint(100)
,`ticketDetails` varchar(500)
,`watchMode` int(1)
,`gensetCapacity` varchar(500)
,`initialTrans` varchar(50)
,`v_d_event1` double
,`v_d_event2` double
,`v_d_event3` double
,`v_d_event4` double
,`location` varchar(500)
,`todayOdometer` bigint(10)
,`packetJson` varchar(3000)
,`packetHex` varchar(3000)
,`height` varchar(50)
,`speedLimit` int(4)
,`fatigueDuration` varchar(500)
,`region` varchar(500)
,`model` varchar(500)
,`ignitionLastUpdate` datetime
,`overSpeedRange` varchar(100)
,`assetInformation` varchar(15000)
,`operatorName` varchar(60)
,`temp1LastTransmission` datetime
);

-- --------------------------------------------------------

--
-- Structure for view `vw_vehicle`
--
DROP TABLE IF EXISTS `vw_vehicle`;

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `vw_vehicle`  AS  select `vehicle`.`companyId` AS `companyId`,`vehicle`.`branchId` AS `branchId`,`vehicle`.`userId` AS `userId`,`vehicle`.`vin` AS `vin`,`vehicle`.`plateNo` AS `plateNo`,`vehicle`.`vehicletype_vehicleType` AS `vehicletype_vehicleType`,`vehicle`.`vehicleGroup` AS `vehicleGroup`,`vehicle`.`vehiclemodel_model` AS `vehiclemodel_model`,`vehicle`.`dateOfPurchase` AS `dateOfPurchase`,`vehicle`.`defaultwarrantyExpiryDate` AS `defaultwarrantyExpiryDate`,`vehicle`.`regNo` AS `regNo`,`vehicle`.`regDate` AS `regDate`,`vehicle`.`regExpiryDate` AS `regExpiryDate`,`vehicle`.`insuranceNo` AS `insuranceNo`,`vehicle`.`insuranceExpiryDate` AS `insuranceExpiryDate`,`vehicle`.`opreAre` AS `opreAre`,`vehicle`.`mileageInit` AS `mileageInit`,`vehicle`.`mileageCur` AS `mileageCur`,`vehicle`.`imgUrl` AS `imgUrl`,`vehicle`.`iconUrl` AS `iconUrl`,`vehicle`.`additionalWarranty` AS `additionalWarranty`,`vehicle`.`geoCnt` AS `geoCnt`,`vehicle`.`lastUpdBy` AS `lastUpdBy`,`vehicle`.`lastUpdDt` AS `lastUpdDt`,`vehicle`.`ioSetting` AS `ioSetting`,`vehicle`.`contactNo` AS `contactNo`,`vehicle`.`fuelTanklit` AS `fuelTanklit`,`vehicle`.`fuelTankmV` AS `fuelTankmV`,`vehicle`.`ioDetails` AS `ioDetails`,`vehicle`.`status` AS `status`,`vehicle`.`hourMeterValue` AS `hourMeterValue`,`vehicle`.`capacity` AS `capacity`,`vehicle`.`expectedMileage` AS `expectedMileage`,`vehicle`.`active` AS `active`,`vehicle`.`lockStatus` AS `lockStatus`,`vehicle`.`PreventiveMaintenance` AS `PreventiveMaintenance`,`vehicle`.`ScheduledMaintenance` AS `ScheduledMaintenance`,`vehicle`.`restDay` AS `restDay`,`vehicle`.`commandsJson` AS `commandsJson`,`vehicle`.`overAllcmdStatus` AS `overAllcmdStatus`,`vehicle`.`reverseSetting` AS `reverseSetting`,`vehicle`.`hierarchyStructure` AS `hierarchyStructure`,`vehicle`.`v_timeStamp` AS `v_timeStamp`,`vehicle`.`v_heartBeatEngine` AS `v_heartBeatEngine`,`vehicle`.`v_gps` AS `v_gps`,`vehicle`.`v_gsm` AS `v_gsm`,`vehicle`.`v_batteryVoltage` AS `v_batteryVoltage`,`vehicle`.`v_powerSupply` AS `v_powerSupply`,`vehicle`.`v_remarks` AS `v_remarks`,`vehicle`.`v_eventTimeStamp` AS `v_eventTimeStamp`,`vehicle`.`v_serverTimeStamp` AS `v_serverTimeStamp`,`vehicle`.`v_latitude` AS `v_latitude`,`vehicle`.`v_longitude` AS `v_longitude`,`vehicle`.`v_speed` AS `v_speed`,`vehicle`.`v_priority` AS `v_priority`,`vehicle`.`v_ioevent` AS `v_ioevent`,`vehicle`.`v_bytesTrx` AS `v_bytesTrx`,`vehicle`.`v_Engine` AS `v_Engine`,`vehicle`.`v_tempSensor1` AS `v_tempSensor1`,`vehicle`.`v_tempSensor2` AS `v_tempSensor2`,`vehicle`.`v_tempSensor3` AS `v_tempSensor3`,`vehicle`.`v_tempSensor4` AS `v_tempSensor4`,`vehicle`.`v_odometer` AS `v_odometer`,`vehicle`.`v_battery` AS `v_battery`,`vehicle`.`v_direction` AS `v_direction`,`vehicle`.`v_eventSource` AS `v_eventSource`,`vehicle`.`v_AI1` AS `v_AI1`,`vehicle`.`v_AI2` AS `v_AI2`,`vehicle`.`v_AI3` AS `v_AI3`,`vehicle`.`v_AI4` AS `v_AI4`,`vehicle`.`v_DI1` AS `v_DI1`,`vehicle`.`v_DI2` AS `v_DI2`,`vehicle`.`v_DI3` AS `v_DI3`,`vehicle`.`v_DI4` AS `v_DI4`,`vehicle`.`v_tags` AS `v_tags`,`vehicle`.`contractor` AS `contractor`,`vehicle`.`vehiclestatus` AS `vehiclestatus`,`vehicle`.`PreventiveMaintenanceType` AS `PreventiveMaintenanceType`,`vehicle`.`vehicleStatusType` AS `vehicleStatusType`,`vehicle`.`EngineHours` AS `EngineHours`,`vehicle`.`group_color` AS `group_color`,`vehicle`.`eventStatus` AS `eventStatus`,`vehicle`.`statusDuration` AS `statusDuration`,`vehicle`.`totalEngineHours` AS `totalEngineHours`,`vehicle`.`ticketDetails` AS `ticketDetails`,`vehicle`.`watchMode` AS `watchMode`,`vehicle`.`gensetCapacity` AS `gensetCapacity`,`vehicle`.`initialTrans` AS `initialTrans`,`vehicle`.`v_d_event1` AS `v_d_event1`,`vehicle`.`v_d_event2` AS `v_d_event2`,`vehicle`.`v_d_event3` AS `v_d_event3`,`vehicle`.`v_d_event4` AS `v_d_event4`,`vehicle`.`location` AS `location`,`vehicle`.`todayOdometer` AS `todayOdometer`,`vehicle`.`packetJson` AS `packetJson`,`vehicle`.`packetHex` AS `packetHex`,`vehicle`.`height` AS `height`,`vehicle`.`speedLimit` AS `speedLimit`,`vehicle`.`fatigueDuration` AS `fatigueDuration`,`vehicle`.`region` AS `region`,`vehicle`.`model` AS `model`,`vehicle`.`ignitionLastUpdate` AS `ignitionLastUpdate`,`vehicle`.`overSpeedRange` AS `overSpeedRange`,`vehicle`.`assetInformation` AS `assetInformation`,`vehicle`.`operatorName` AS `operatorName`,`vehicle`.`temp1LastTransmission` AS `temp1LastTransmission` from `vehicle` ;

