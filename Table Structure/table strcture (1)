  `vehicle_manufacturer_arabic` varchar(150) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_model_year` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_plate_left_letter` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_plate_middle_letter` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_plate_number` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_plate_right_letter` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_reference_key` varchar(250) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_rejection_reason` varchar(250) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_status` varchar(50) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `vehicle_validity_expiry` varchar(20) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL,
  `velocity` varchar(50) DEFAULT NULL,
